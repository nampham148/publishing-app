<%= content_tag :div, id: "interact-form-#{post.id}" do %>
  <% if current_user.like?(post) %>
    <%= render 'posts/liked_interact_form', post: post %>
  <% elsif current_user.dislike?(post) %>
    <%= render 'posts/disliked_interact_form', post: post %>
  <% else %>
    <%= render 'posts/neutral_interact_form', post: post %>
  <% end %>
<% end %>